<views:ViewBase x:Class="AE.PID.Views.Components.DocumentInfoTable"
                x:TypeArguments="viewModels:DocumentInfoViewModel"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                xmlns:views="clr-namespace:AE.PID.Views"
                xmlns:viewModels="clr-namespace:AE.PID.ViewModels"
                xmlns:controls="clr-namespace:AE.PID.Views.Controls"
                mc:Ignorable="d"
                d:DataContext="{d:DesignInstance Type=viewModels:DocumentInfoViewModel}"
                d:DesignHeight="52" d:DesignWidth="600">
    <UniformGrid Columns="2">
        <controls:LabelTextBox x:Name="CustomerNameInput" Label="客户名称" />
        <controls:LabelTextBox x:Name="DocumentNoInput" Label="文档编号" />
        <controls:LabelTextBox x:Name="ProjectNoInput" Label="项目编号" />
        <controls:LabelTextBox x:Name="VersionNoInput" Label="版本号" />
    </UniformGrid>
</views:ViewBase>